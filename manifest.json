{
  "manifest_version": 3,
  "name": "No Leetcode CN Redirect",
  "version": "1.2",
  "description": "Stop auto redirecting to leetcode.cn",
  "icons": {
    "16": "images/logo-16x16.png",
    "32": "images/logo-32x32.png",
    "48": "images/logo-48x48.png",
    "128": "images/logo-128x128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://leetcode.com/*"
      ]
    }
  ]
}